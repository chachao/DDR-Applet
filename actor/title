//import
import greenfoot.*;  // (World, Actor, GreenfootImage, Greenfoot and MouseInfo)
import java.awt.*;

/**
  * The Title class is the DDR Extreme title
 */
public class Title extends Actor
{

    private GreenfootImage image1 = new GreenfootImage("title.png");
    
    //constructor
    public Title(){
        
        image1.scale(300, 250);
        setImage(image1);
    }
  
}
